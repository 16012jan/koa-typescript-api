{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "array-type": [true, "generic"],
    "ordered-imports": false,
    "no-console": [true, "info", "warn", "error"]
  },
  "rulesDirectory": []
}